
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>tiempoPromedioCajero</title>
</head>
<body>
    <h3>Promedio de Cajeros</h3>
    <table class="table table-bordered" cellpadding="2" cellspacing="2" border="1">
        <tr class="info">
            <th>Nombre_persona</th>
            <th>Apellido1</th>
            <th>Apellido2</th>
            <th>minutos_por_factura</th>
            <th>cantidad_unidades</th>

        </tr>

        @foreach (var p in ViewBag.ListProducts)
            {
            <tr>
                <td>@p.Nombre_persona</td>
                <td>@p.Apellido1</td>
                <td>@p.Apellido2</td>
                <td>@p.minutos_por_factura</td>
                <td>@p.cantidad_unidades</td>
            </tr>
        }

    </table>  
</body>
</html>
